    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "eslint": "^9.0.0",
    "eslint-config-dsfx3d": "^0.1.5",
    "husky": "^9.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
